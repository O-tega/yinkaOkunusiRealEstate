    "formik": "^2.4.6",
    "framer-motion": "^11.2.6",
    "husky": "^8.0.3",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-cookies": "^0.1.1",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.5",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.2.1",
    "react-loading-skeleton": "^3.5.0",
    "react-multi-carousel": "^2.8.5",
    "react-router-dom": "^6.23.1",
    "react-router-hash-link": "^2.4.3",
    "react-select": "^5.8.0",
